<div class="row">
  <div class="col">
    <h1>Ideas</h1>
    <% @ideas.each do |idea| %>
      <p><%= link_to idea.idea_name, idea %></p>
    <% end %>
  </div>

  <div class="col">
    <h1>Teams</h1>
    <% @teams.each do |team| %>
      <p><%= link_to team.team_name, team %></p>
    <% end %>
  </div>
</div>
