<h1>Users</h1>

<div class="row row-cols-1 row-cols-md-5">
  <% @users.each do |user| %>
    <div class="col mb-4">
      <div class="card border-dark h-100">
        <div class="card-body">
          <p class="card-text"><%=link_to user.first_name, user %> <%= link_to user.last_name, user  %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
