<body>
<main>
  <h1>Ideas</h1>
</main>

<div class="row">
  <table class="table table-bordered table-hover">
    <thead class="thead-light">
    <div class="col s12 m10 offset-m1 l9 offset-l1">
      <div class="post-preview">
        <h4 class="post-title titleFont">
          <% @ideas.each do |record| %>
            <tr>
              <td style="width: 5%"></td>
              <td><%= link_to record.idea_name, idea_path(record), method: :get %></td>
              <td style="width: 5%">
              </td>
              <td style="width: 5%">
              </td>
            </tr>
          <% end %>
        </h4>
      </div>
      <hr>
    </div>
  </table>
</div>
</div>
</body>
