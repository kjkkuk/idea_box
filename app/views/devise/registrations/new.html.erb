<div class="row-registarion">
  <div class="wrapper-form">
      <%= form_for(resource, as: resource_name, url: registration_path(resource_name), method: :post, html: { class: 'col s12', id: 'reg-form'}) do |f| %>
        <div class="row">
          <div class="input-field col s6">
            <i class="material-icons prefix">group</i>
            <%= f.text_field :first_name, class: "validate", required: true %>
            <%= f.label :first_name, 'First name' %>
          </div>
          <div class="input-field col s6">
            <%= f.text_field :last_name, class: "validate", required: true %>
            <%= f.label :last_name, 'Last Name' %>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">mail_outline</i>
            <%= f.text_field :email, class: "validate", required: true %>
            <%= f.label :email, 'Email' %>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s12">
            <i class="material-icons prefix">lock_outline</i>
            <%= f.password_field :password, class: "validate", required: true %>
            <%= f.label :password, 'Password' %>
          </div>
        </div>
        <div class="row">
          <div class="input-field col s6">
            <%= f.submit 'Register', class: 'btn btn-outline-primary' %>
          </div>
          <div class="input-field col s6">
            <%= link_to 'Back', :back, class: 'btn btn-large btn-outline-secondary'%>
          </div>
        </div>
      <% end %>
</div>
<div class="wrapper-links"
      <%= render "devise/shared/links" %>
</div>
</div>
